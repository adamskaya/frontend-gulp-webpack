{
  "env": {
    "browser": true,
    "node": true
  },
  "extends": [
    "tslint:recommended",
    "tslint-eslint-rules"
  ],
  "jsRules": {},
  "rules": {
    "quotemark" : [true, "single"],
    "max-line-length": [true, 200],
    "member-access": [true, "no-public"],
    "no-console": false,
    "adjacent-overload-signatures": true,
    "trailing-comma": false,
    "new-parens": true,
    "no-arg": true,
    "no-bitwise": true,
    "no-conditional-assignment": true,
    "no-consecutive-blank-lines": false,
    "indent": false,
    "variable-name": false
  },
  "rulesDirectory": []
}