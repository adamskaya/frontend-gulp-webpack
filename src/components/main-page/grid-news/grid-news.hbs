<grid-element data-count-query = "(max-width:700px)=>1|(max-width:1100px)=>2 |3 ">
    <section class="articles" data-items>
        {{> list-news list=news}}
        {{> component-news var=news.[0]}}
        {{> component-news var=news.[1]}}
        {{> component-news var=news.[2]}}
        {{> component-news var=news.[3]}}
        {{> component-news var=news.[4]}}
    </section>
</grid-element>