{
  "env": {
    "browser": true,
    "node": true
  },
  "extends": [
    "tslint:recommended",
    "tslint-eslint-rules"
  ],
  "jsRules": {},
  "rules": {
    "no-undef": "off",
    "import/no-named-as-default":0,
    "function-paren-newline": ["error", "consistent"],
    "object-curly-newline": ["error", { "consistent": true }],
    "no-constant-condition": true
  },
  "rulesDirectory": []
}